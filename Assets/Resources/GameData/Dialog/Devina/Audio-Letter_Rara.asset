    speakerName: Rara
    text: Jadi aku berangkat sendirian. Maafkan aku Devina.
    voiceClip: {fileID: 0}
    duration: 3
  - id: 10
    speakerName: Rara
    text: Aku percaya yang membaca surat ini engkau Devina.
    voiceClip: {fileID: 0}
    duration: 3
