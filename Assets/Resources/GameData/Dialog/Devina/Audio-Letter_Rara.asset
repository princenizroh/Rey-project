    text: Lagi-lagi kamu nekat sendirian Rara.
    voiceClip: {fileID: 0}
    duration: 3
  - id: 12
    speakerName: Devina
    text: Jika bertemu akan kupukul kamu.
    voiceClip: {fileID: 0}
    duration: 3
